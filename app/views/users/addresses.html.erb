<%= render "header" %>
<% if flash['success'] %>
  <%= javascript_tag do %>
      Materialize.toast("<%= flash['success'] %>", 4000)
  <% end %>
<% end %>
<div class="signin-box">
  <div class="container account-container">
    <div class="row">
      <div class= "col s3">
        <%= render "account_side_bar" %>

      </div>
       <div class= "col s9">
        <div class="profile">

          <div class="result-header">
            Manage Address
          </div>
          <div class="row profile-fields">
          <%= display_addresses %>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<%= render "footer" %>
